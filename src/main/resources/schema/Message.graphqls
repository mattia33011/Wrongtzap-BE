extend type Query{
    Message(messageId: String!): Message!
    searchMessage(messageBody: String!): [Message!]
    everyMessage: [Message!]
}

type Message{
    messageId: String!
    timestamp: Float!
    content: String!
    username: String!
    userId: String!
    associatedChat: String!
    deletedForUser: [String]!
}

type MessageResponse{
    username: String!
    userId: String!
    content: String!
    timestamp: Float!
}

