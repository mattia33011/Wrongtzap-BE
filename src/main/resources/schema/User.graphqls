type Query{
    user(userId: String!): User!
    everyUser: [User!]
}

type User {
    userId: String!
    username: String!
    chats: [Chat!]
    friends: [UserProfile!]
}

type Profile{
    username: String!
    userId: String!
}

type UserProfile{
    userId: String!
    username: String!
}




